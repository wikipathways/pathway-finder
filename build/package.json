{
  "name": "pathway-finder",
  "version": "0.0.0",
  "description": "Your query list -> WikiPathways results, by means of mapping DBs.",
  "main": "main.js",
  "main:esnext": "esnext/main.js",
  "files": [
    "index.d.ts",
    "main.js",
    "main.d.ts",
    "esnext/"
  ],
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:ts": "npm-run-all -p build:ts:*",
    "build:ts:es5": "tsc --project tsconfig.es5.json",
    "build:ts:esnext": "tsc --project tsconfig.esnext.json",
    "postinstall": "npm run build && node ./es5/build-hash.js",
    "start-old": "npm-run-all -p start:mockserver start:webpack",
    "start-old:mockserver": "echo 'Do we need mockserver anymore?' &&./node_modules/mockserver/bin/mockserver.js -p 4522 -m './test/mockserver-data'",
    "start": "npm-run-all -p start:*",
    "start:notice": "echo 'For demo, open https://www.wikipathways.org/wpi/extensions/PathwayFinder/?database=mirtarbase&identifiers=Example%20miRNA%20target%20search:,hsa-miR-148a-3p,hsa-miR-21-3p,hsa-miR-29a-3p,hsa-miR-20a-5p'",
    "start:webpack": "webpack-dev-server --config webpack.config.js --progress --colors --watch",
    "test": "echo 'add working tests'",
    "watch-old": "npm-run-all -p start:mockserver watch:*",
    "watch-old:client": "echo 'What is watch:client? Is it outdated/to be deleted?' && npm-watch",
    "watch": "npm run start"
  },
  "watch": {
    "test": [
      "main.js",
      "test/*.{js,jsx}"
    ]
  },
  "browser": {
    "commander": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wikipathways/pathway-finder.git"
  },
  "keywords": [
    "pathway-finder",
    "network",
    "pathway",
    "biology"
  ],
  "author": "WikiPathways team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wikipathways/pathway-finder/issues"
  },
  "homepage": "https://wikipathways.github.io/pathway-finder/docs/",
  "dependencies": {
    "@types/lodash": "^4.14.36",
    "@types/node": "^6.0.101",
    "@types/react": "^15.0.5",
    "@types/react-dom": "^0.14.18",
    "JSONStream": "^1.3.2",
    "csstips": "^0.2.0",
    "csv-streamify": "^1.0.0",
    "csx": "^8.2.1",
    "file-exists": "^3.0.2",
    "lodash": "^4.14.36",
    "material-ui": "^0.17.1",
