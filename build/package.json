  },
  "semverBumper": {
    "jsonFiles": [
      {
        "filepath": "package.json",
        "key": "version"
      }
    ],
    "grepOptions": {
      "include": [],
      "exclude": [
        ".git",
        "jsdoc-conf.json",
        "npm-debug.log"
      ],
      "excludeDir": [
        "./docs",
        "./test",
        "./bower_components",
        "./lib",
        "./demo",
        "./gulp",
        "./dist"
      ]
    }
  }
